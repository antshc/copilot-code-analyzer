root = true

[*]
end_of_line = crlf
indent_style = space
indent_size = 2
[*.cs]
indent_size = 4
# should be removed after stlye cope task finish
dotnet_diagnostic.IDE0130.severity = none
# should be removed after stlye cope task finish

# var preferences
csharp_style_var_for_built_in_types = false:error
csharp_style_var_when_type_is_apparent = true:error
csharp_style_var_elsewhere = false:suggestion

csharp_style_implicit_object_creation_when_type_is_apparent  = false

csharp_style_expression_bodied_methods = when_on_single_line
csharp_style_namespace_declarations = file_scoped:error
csharp_preserve_single_line_statements = false

# IDE0078: Use pattern matching
csharp_style_prefer_pattern_matching = false

resharper_inconsistent_naming_highlighting=suggestion

#Enable on build analyzer diagnostics
dotnet_analyzer_diagnostic.category-Style.severity = error
dotnet_analyzer_diagnostic.category-Naming.severity = warning

# Remove unnecessary using directives
dotnet_diagnostic.IDE0005.severity = error

# Use auto-implemented property (IDE0032)
dotnet_diagnostic.IDE0032.severity = suggestion

# Use explicitly provided tuple name
dotnet_diagnostic.IDE0033.severity = suggestion

# Use range operator (IDE0057)
dotnet_diagnostic.IDE0057.severity = suggestion

# Use index operator (IDE0056)
dotnet_diagnostic.IDE0056.severity = silent

# IDE0058: Remove unnecessary expression value (disabled on build)
dotnet_diagnostic.IDE0058.severity = silent

# Use expression body for local functions
dotnet_diagnostic.IDE0061.severity = suggestion

# Use simple 'using' statement
dotnet_diagnostic.IDE0063.severity = suggestion

# Use switch expression (IDE0066)
dotnet_diagnostic.IDE0066.severity = suggestion

# Convert to top-level statements
dotnet_diagnostic.IDE0210.severity = suggestion

# Struct member can be made 'readonly' (IDE0251)
dotnet_diagnostic.IDE0251.severity = suggestion

# Null check can be simplified
dotnet_diagnostic.IDE0270.severity = suggestion
dotnet_diagnostic.IDE0031.severity = suggestion

# CA1716: Using a reserved keyword as the name of a virtual/interface member makes it harder for consumers in other languages to override/implement the member (disabled on build)
dotnet_diagnostic.CA1716.severity = silent

# CA1711: Identifiers should not have incorrect suffix
dotnet_diagnostic.CA1711.severity = none

# CA1725: Parameter names should match base declaration
dotnet_diagnostic.CA1725.severity = silent

# Use PascalCase for named placeholders
dotnet_diagnostic.CA1727.severity = suggestion

# Simplify name (IDE0001)
dotnet_diagnostic.IDE0001.severity = error

# IDE0007 Use var instead of explicit type
dotnet_diagnostic.IDE0007.severity = suggestion

# IDE0008 Use explicit type instead of var
dotnet_diagnostic.IDE0008.severity = error

# Add missing cases to switch statement (IDE0010)
dotnet_diagnostic.IDE0010.severity = suggestion
dotnet_diagnostic.IDE0072.severity = suggestion

# Use compound assignment 
dotnet_diagnostic.IDE0074.severity = suggestion

# Use object initializers
dotnet_diagnostic.IDE0017.severity = suggestion

# IDE0022: Use expression body for methods (disabled on build)
dotnet_diagnostic.IDE0022.severity = silent

# Use expression body for operators (IDE0023 and IDE0024)
dotnet_diagnostic.IDE0023.severity = silent
dotnet_diagnostic.IDE0024.severity = silent

# Use 'is null' check (IDE0041)
dotnet_diagnostic.IDE0041.severity = silent

# Deconstruct variable declaration
dotnet_diagnostic.IDE0042.severity = suggestion

# Add readonly modifier
dotnet_diagnostic.IDE0044.severity = error

# Use conditional expression for assignment
dotnet_diagnostic.IDE0045.severity = suggestion

# Use conditional expression for return (IDE0046)
dotnet_diagnostic.IDE0046.severity = suggestion

# Parentheses preferences (IDE0047 and IDE0048)
dotnet_diagnostic.IDE0047.severity = suggestion
dotnet_diagnostic.IDE0048.severity = suggestion

# Use language keywords instead of framework type names for type references (IDE0049)
dotnet_diagnostic.IDE0049.severity = suggestion

# Use compound assignment
dotnet_diagnostic.IDE0054.severity = suggestion

# Formatting rule
dotnet_diagnostic.IDE0055.severity = error

# Simplify LINQ expression (IDE0120)
dotnet_diagnostic.IDE0120.severity = suggestion

###############################
# Naming Conventions          #
###############################
# Style Definitions
dotnet_naming_style.pascal_case_style_types.capitalization = pascal_case

dotnet_naming_symbols.pascal_case_style_types_symbols.applicable_kinds = class, struct, interface, enum, delegate
dotnet_naming_symbols.pascal_case_style_types_symbols.applicable_accessibilities = *

dotnet_naming_rule.pascal_case_types.style = pascal_case_style_types
dotnet_naming_rule.pascal_case_types.symbols = pascal_case_style_types_symbols
dotnet_naming_rule.pascal_case_types.severity = error

# Use c_camelCase for private constant fields  
dotnet_naming_rule.private_constant_fields_should_be_camel_case.severity = error
dotnet_naming_rule.private_constant_fields_should_be_camel_case.symbols = private_constant_fields
dotnet_naming_rule.private_constant_fields_should_be_camel_case.style = c_camel_case_style
dotnet_naming_symbols.private_constant_fields.applicable_kinds = field
dotnet_naming_symbols.private_constant_fields.applicable_accessibilities = private
dotnet_naming_symbols.private_constant_fields.required_modifiers = const

dotnet_naming_rule.protected_constant_fields.severity = error
dotnet_naming_rule.protected_constant_fields.symbols = protected_constant_fields
dotnet_naming_rule.protected_constant_fields.style = c_camel_case_style
dotnet_naming_symbols.protected_constant_fields.applicable_kinds = field
dotnet_naming_symbols.protected_constant_fields.applicable_accessibilities = protected
dotnet_naming_symbols.protected_constant_fields.required_modifiers = const

dotnet_naming_style.c_camel_case_style.capitalization = camel_case
dotnet_naming_style.c_camel_case_style.required_prefix = c_

# Use PascalCase for public constant fields  
dotnet_naming_symbols.public_constant_fields.applicable_kinds = field
dotnet_naming_symbols.public_constant_fields.applicable_accessibilities = public, internal
dotnet_naming_symbols.public_constant_fields.required_modifiers = const
dotnet_naming_rule.public_constant_fields_should_be_pascal_case.severity = error
dotnet_naming_rule.public_constant_fields_should_be_pascal_case.symbols = public_constant_fields
dotnet_naming_rule.public_constant_fields_should_be_pascal_case.style = pascal_case_style_types

# Use m_camelCase for private fields  
dotnet_naming_rule.camel_case_for_private_fields.severity = error
dotnet_naming_rule.camel_case_for_private_fields.symbols = private_fields
dotnet_naming_rule.camel_case_for_private_fields.style = m_camel_case_style
dotnet_naming_symbols.private_fields.applicable_kinds = field
dotnet_naming_symbols.private_fields.applicable_accessibilities = private

dotnet_naming_style.m_camel_case_style.required_prefix = m_
dotnet_naming_style.m_camel_case_style.capitalization = camel_case
# Use s_camelCase for private static fields  
dotnet_naming_rule.camel_case_for_private_static_fields.severity = error
dotnet_naming_rule.camel_case_for_private_static_fields.symbols = static_fields
dotnet_naming_rule.camel_case_for_private_static_fields.style = s_camel_case_style
dotnet_naming_symbols.static_fields.applicable_kinds = field
dotnet_naming_symbols.static_fields.applicable_accessibilities = private
dotnet_naming_symbols.static_fields.required_modifiers = static

dotnet_naming_style.s_camel_case_style.required_prefix = s_
dotnet_naming_style.s_camel_case_style.capitalization = camel_case

# Disable NET 8 formating warnings
dotnet_diagnostic.IDE0100.severity = silent
dotnet_diagnostic.IDE0200.severity = silent
dotnet_diagnostic.IDE0290.severity = silent
dotnet_diagnostic.IDE0300.severity = silent
dotnet_diagnostic.IDE0301.severity = silent
dotnet_diagnostic.IDE0303.severity = silent
dotnet_diagnostic.IDE0305.severity = silent

## REGION Applied StyleCop formatting 

# A single-line comment within C# code is followed by a blank line.
# https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1512.md
dotnet_diagnostic.SA1512.severity = silent

# The line endings at the end of a file do not match the settings for the project.
# https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1518.md
dotnet_diagnostic.SA1518.severity = error

# The spacing around an operator symbol is incorrect, within a C# code file.
# https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1003.md
dotnet_diagnostic.SA1003.severity = error

# A line of code ends with a space, tab, or other whitespace characters before the end of line character(s).
# https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1028.md
dotnet_diagnostic.SA1028.severity = error

# Adjacent C# elements are not separated by a blank line.
# https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1516.md
dotnet_diagnostic.SA1516.severity = error

# A closing brace within a C# element, statement, or expression is preceded by a blank line.
# https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1508.md
dotnet_diagnostic.SA1508.severity = error

# The C# code contains multiple blank lines in a row.
# https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1507.md
dotnet_diagnostic.SA1507.severity = error

# A parameter to a C# method or indexer, other than the first parameter, spans across multiple lines.
# https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1118.md
dotnet_diagnostic.SA1118.severity = suggestion

# An attribute is placed on the same line of code as another attribute or element.
# https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/SA1134.md
dotnet_diagnostic.SA1134.severity = error

